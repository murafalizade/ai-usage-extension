{
  "manifest_version": 3,
  "name": "AI Usage Tracker",
  "version": "1.0",
  "description": "Tracks time spent on ai based assistants",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  }
}
